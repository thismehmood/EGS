<%= render "/layouts/landing"%>
  <div class="page-header" data-parallax="true" style="background-image: url('https://wonderfulengineering.');">
    <div class="filter"></div>
     <div class="container">
        <div class="motto text-center"> 
         <h1>Corporative Event affairs</h1>
        </div> 
         <div class="row justify-content-center">
            <% if user_signed_in? %>  
              <%if current_user.role == "venue_owner"%>
                <div class="card" style="width: 15rem; margin-top: 20px; margin-left: 5px;">
                  <img class="card-img-top" src="https://bangkokpartyrentals.com/wp-content/uploads/2018/10/25.jpg" alt="Card image cap">
                 <div class="card-body">
                   <h4 class="card-title">becomes an venue Partner</h4>
                   <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                   <a href="venue_owner/venues" class="btn btn-primary btn-sm">Request venue</a>
                 </div>
                </div>
              <% else %>
              <div class="card" style="width:  15rem; margin-top: 20px;">
                  <img class="card-img-top" src="/assets/daniel-olahs.jpg" alt="Card image cap">
                  <div class="card-body">      
                    <h4 class="card-title">Request An Event</h4>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    <a href=<%= customers_venues_path%> class="btn btn-primary btn-sm">Search For Event</a>
                  </div>
               </div>
              <% end %>
            <% end %>     
        </div>
      </div> 
    </div>
  </div>

  <div class="main" style="display: none1;">
    <div class="section text-center">
      <div class="container">
        <div class="row">
          <div class="col-md-8 ml-auto mr-auto">
            <h2 class="title">Let's talk product</h2>
