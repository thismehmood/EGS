<div class="page-header" data-parallax="true" style="background-image: url('https://wonderfulengineering.');">
 <div class="container">
   <%= render "/layouts/landing"%>
   <div class="row">
     <div class="col-10">
       <h2 style="margin: 0;">venues</h2>
     </div>
     <div class="col-12">
       <a class="btn btn-success float-right btn-sm margin-top = 35px"  href=<%= new_venue_owner_venue_path%> > Create_venue </a>
       <a class="btn btn-success float-right btn-sm margin-top = 35px" href=<%= venue_owner_bookings_path%> > All_Bookings </a>
     </div>
   </div>
    <div  style = "position:relative; height:400px; overflow:auto; display:block;">
      <table class="table table-bordered table-striped md-0 table-hover table-dark">
       <thead>
         <tr>
           <th>#</th>
           <th> VenueOwner ID</th>
           <th>Name</th> 
           <th>Address</th>
           <th>Venue type</th>
           <th>Contact no</th>
           <th> Action</th>
          </tr>
       </thead>
       <tbody>
         <% @venue.each_with_index do |q|%> 
           <tr>
             <td><%= q.id%></td>
             <td><%= q.user.id%>
             <td><%= q.name%></td>
             <td><%= q.address%></td>
             <td><%= q.venue_type%></td>
             <td><%= q.contact_no%></td>
             <td style="padding:0px;"><a class="btn btn-success float-right btn-sm" href="/venue_owner/venues/<%= q.id%>/edit"> Edit </a></td>
             <td><%= link_to 'venue_destroy', venue_owner_venue_path(q), method: :delete, data: { confirm: 'Are you sure?' }%> </td>
           </tr>
         <%end%>   
       </tbody>
      </table>
    </div>
  </div>
</div>