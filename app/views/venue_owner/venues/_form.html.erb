      
  <%= form_with(model: venue, url: venue_owner_venues_path(venue), local: true) do |form| %>
           <div class="form-group">
              <%= label(:venue, :name)%>
              <%= form.text_field :name ,class: "form-control"%>
          </div>
            <div class="form-group">
              <%= label(:venue, :address)%>
              <%= form.text_field :address ,class: "form-control"%>
             </div>
           <div class="form-group">
            <%= label(:venue, :venue_type)%>
              <%= form.select :venue_type, ['Hotel', 'Resturant','Mariage Palace'],{},{ class: "form-control"}%>
           </div>
           <div class="form-group">
              <%= label(:venue, :contact_no)%>
              <%= form.text_field :contact_no ,class: "form-control"%>
           </div>

               <%# <%= form.hidden_field :user_id %> 

           <div class = "form-group">
               <%= form.submit 'Submit', class: "btn btn-lg btn-primary" %>   
           </div>          
   <% end %>